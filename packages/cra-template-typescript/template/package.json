{
  "name": "template-cra-template-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "lint:style": "stylelint **/*.{css,less,scss}",
    "fix:style": "npm run lint:style -- --fix",
    "lint:script": "eslint --ext .tsx --ext .ts src/",
    "fix:script": "npm run lint:script -- --fix",
    "fix": "npm run fix:script & npm run fix:style",
    "prettier": "prettier \"**/*\" -c -w -u",
    "precommit": "npm run fix && npm run prettier"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "tailwindcss": "^3.3.5",
    "web-vitals": "^2.1.4",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@dsaco/eslint-config": "^1.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "craco-less": "^3.0.1",
    "eslint": "^8.54.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "http-proxy-middleware": "^2.0.6",
    "less": "^4.2.0",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "prettier": "3.1.0",
    "react-scripts": "5.0.1",
    "sass": "^1.69.5",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {}
}

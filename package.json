{
  "name": "ds-packages",
  "version": "1.0.0",
  "description": "dsaco packages",
  "license": "ISC",
  "author": "dsaco",
  "scripts": {
    "start:cra": "pnpm -C packages/cra-template/template start",
    "publish:cra": "npm publish ./packages/cra-template",

    "start:cra-ts": "pnpm -C packages/cra-template-typescript/template start",
    "publish:cra-ts": "npm publish ./packages/cra-template-typescript",
    
    "start:cra-admin": "pnpm -C packages/cra-template-admin/template start",
    "publish:cra-admin": "npm publish ./packages/cra-template-admin",

    "publish:eslint-config": "npm publish ./packages/eslint-config",

    "start:utils": "pnpm -C packages/utils start",
    "test:utils": "pnpm -C packages/utils test",
    "build:utils": "pnpm -C packages/utils build",
    "publish:utils": "npm publish ./packages/utils",

    "start:rc": "pnpm -C packages/react-components start",
    "test:rc": "pnpm -C packages/react-components test",
    "build:rc": "pnpm -C packages/react-components build",
    "publish:rc": "npm publish ./packages/react-components"
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "prettier-plugin-package": "^1.3.0"
  }
}
